                        )
                      )
                    ]
                  , [L2E(V2L(Var("c")))]
                  )
                )
              ]
            )
          )
        ]
      )
    , VarDecNoType("r", Call("fact", [Int("10")]))
    ]
  , [L2E(V2L(Var("r")))]
  )
)
